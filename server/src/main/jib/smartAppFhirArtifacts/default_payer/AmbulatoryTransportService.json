     {
       "linkId": "8",
       "text": "Additional Documentation Required",
       "type": "group",
       "item": [
	 {
           "linkId": "8.1",
           "text": "Physician Certification Statement  indicating the origin and/or destination of the transport",
           "type": "attachment",
           "required": true,
           "code": {
             "coding": [
               {
                 "system": "http://loinc.org",
                 "code": "LP74233-5",
                 "display": "Physician certification statement"
               },
               {
                 "system": "http://loinc.org",
                 "code": "52016-3",
                 "display": "Ambulance transport, Physician certification for transport statement (narrative)"
               }
             ]
           }
         },
         {
           "linkId": "8.2",
           "text": "Note: The medical documentation must describe how the specific structural and/or functional impairments together with the activity limitations contribute to the determination that stretcher transport is necessary for the patient's safety as it relates to their condition",
