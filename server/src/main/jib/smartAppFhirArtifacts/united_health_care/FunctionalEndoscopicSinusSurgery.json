      "type": "group",
      "item": [
        {
          "linkId": "3.1",
          "text": "Primary Diagnosis[ICD-10]",
          "type": "open-choice",
          "required": true,
          "repeats": true,
          "option": [
            {
              "valueCoding": {
                "code": "Chronic Rhinosinusitis (CRS) which has lasted longer than 12 weeks"
              }
            },
            {
              "valueCoding": {
                "code": "Recurrent Acute Rhinosinusitis"
              }
            },
            {
              "valueCoding": {
                "code": "Mucocele"
              }
            },
            {
              "valueCoding": {
                "code": "Concha bullosa"
              }
            },
            {
              "valueCoding": {
                "code": "Complications of sinusitis such as abscess"
              }
            },
            {
              "valueCoding": {
                "code": "Tumor confirmed (such as polyposis or malignancy)"
              }
            }
          ],
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqif-calculatedValue",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"OxygenTherapy\".Relevant Diagnoses"
              }
            }
          ]
        },
        {
          "linkId": "3.2",
          "text": "Date of onset of Symptoms",
          "type": "date",
          "required": true,
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqif-calculatedValue",
              "valueString": "DateOfOnset"
            }
          ]
        },
        {
          "linkId": "3.3",
          "text": "Persistence of Symptoms despite following treatments",
          "type": "open-choice",
          "required": true,
          "enable-when": [
            {
              "question": "3.1",
              "answerCoding": {
                "code": "Chronic Rhinosinusitis (CRS) which has lasted longer than 12 weeks"
              }
            }
          ],
          "option": [
            {
              "valueCoding": {
                "code": "Antibiotic therapy, if bacterial infection is suspected"
              }
            },
            {
              "valueCoding": {
                "code": "Nasal lavage"
              }
            },
            {
              "valueCoding": {
                "code": "Intranasal corticosteroids"
              }
            }
          ],
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqif-calculatedValue",
              "valueExpression": {
                "language": "text/cql",
                "expression": "\"OxygenTherapy\".Treatments performed"
              }
            }
          ]
        },
        {
          "linkId": "3.3",
          "text": "Information additional to Recurrent Acute Rhinosinusitis",
          "required": true,
          "type": "string",
          "code": {
            "coding": [
              {
                "valueCoding": {
                  "code": "Sinonasal symptoms"
                }
              },
              {
                "valueCoding": {
                  "code": "CT evidence of ostial occlusion and/or mucosal thickening in the sinus to be treated"
                }
              }
            ]
          },
          "extension": [
            {
              "url": "http://hl7.org/fhir/StructureDefinition/cqif-calculatedValue",
              "valueString": "Additional Info"
            }
          ]
